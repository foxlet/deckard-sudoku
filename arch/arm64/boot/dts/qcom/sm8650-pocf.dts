		"",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",
		// GPIO112 - GPIO127
		"",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",
		// GPIO128 - GPIO143
		"",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",
		// GPIO144 - GPIO159
		"",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",
		// GPIO160 - GPIO175
		"",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",  "",
		// GPIO176 - GPIO180
		"",  "",  "",  "",  "", "", "", "", "", "", "", "", "", "qpd_fpga_dready";

	fpga_clk_pins {
		fpga_clk_default: fpga_clk_default {
			mux {
				pins = "gpio88";
				function = "gcc_gp3";
			};
			config {
				pins = "gpio88";
				drive-strength = <6>;
				bias-pull-none; /* pull none */
			};
		};

		fpga_clk_active: fpga_clk_active {
			mux {
				pins = "gpio88";
				function = "gcc_gp3";
			};
			config {
				pins = "gpio88";
				drive-strength = <2>; /* 2 mA */
				bias-pull-none; /* pull none */
			};
		};

		fpga_clk_sleep: fpga_clk_sleep {
			mux {
				pins = "gpio88";
				function = "gpio";
			};
			config {
				pins = "gpio88";
				drive-strength = <2>; /* 2 mA */
				bias-pull-down; /* pull down */
			};
		};
	};
};


// nRF SPI HID Config

// nRF Pins
&tlmm {
	// nRF CS
	qup_spi10_cs_nrf: qup-spi10-cs-state-nrf {
		pins = "gpio13", "gpio15";
		function = "qup2_se2";
		drive-strength = <6>;
		bias-disable;
	};

	// nRF SPI interrupt and reset pins

    // expansion 
	l_spihid_default_state: l-spihid-default {
		l_irq0 {
			pins = "gpio69";
			function = "gpio";
			bias-pull-up;
		};

		 l_reset0 {
			 pins = "gpio70";
			 function = "gpio";
			 drive-strength = <2>;
			 output-low;
			 bias-pull-down;
		 };
	};

	// host/mainboard
	r_spihid_default_state: r-spihid-default {
		r_irq0 {
			pins = "gpio66";
			function = "gpio";
			bias-pull-up;
		};

		r_reset0 {
			pins = "gpio137";
			function = "gpio";
			drive-strength = <2>;
			output-low;
			bias-pull-down;
		};
	};

	nrf_host_rst_n: nrf_host_rst_n {
		mux {
			pins = "gpio164";
			function = "gpio";
		};

		config {
			pins = "gpio164";
			power-source = <1>; /* 1.8 V */
			bias-disable = <0>;
			output-enable;
			output-low;
		};
	};
};

