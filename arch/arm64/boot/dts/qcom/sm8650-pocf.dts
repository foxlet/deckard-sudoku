/ {
    model = "SM8650 POC-F rev1 4slam 2et";

	interrupt-parent = <&intc>;

	#address-cells = <2>;
	#size-cells = <2>;

	aliases {
		serial0 = &uart15;
		serial1 = &uart14;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	vdd_gfx: vdd-gfx {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_GFX>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "GFX";
		regulator-always-on;
	};

	vdd_mxc: vdd-mxc {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_MXC>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "MXC";
		regulator-always-on;
	};

	vdd_lcx: vdd-lcx {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_LCX>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "LCX";
		regulator-always-on;
	};

	vdd_lmx: vdd-lmx {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_LMX>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "LMX";
		regulator-always-on;
	};

	vdd_nsp: vdd-nsp {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_NSP>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "NSP";
		regulator-always-on;
	};

	vdd_nsp2: vdd-nsp2 {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_NSP2>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "NSP2";
		regulator-always-on;
	};

	vdd_mx: vdd-mx {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_MX>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "MX";
		regulator-always-on;
	};


	VPH_PWR: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "VPH_PWR";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;

		regulator-always-on;
		regulator-boot-on;
	};

	wlan_regulator: wlan-regulator {
		compatible = "regulator-wlan";
		regulator-name = "wlan";

		enable-gpio = <&tlmm 16 GPIO_ACTIVE_HIGH>;

		supply-count = <7>;

		vin0-supply = <&VREG_S3C_0P97>; // VDD_PMU_AON_I / VDD095_PMU_BT / VDD09_PMU_RFA_I
		vin1-supply = <&VREG_S4I_0P75>; // VDD095_PMU
		vin2-supply = <&VREG_S2C_0P85>; // VDD095_MX_PMU
		vin3-supply = <&VREG_S1C_1P25>; // VDD13_PMU_RFA_I / VDD13_PMU_PCIE_I
		vin4-supply = <&VREG_S6C_1P86>; // VDD19_PMU_RFA_I / VDD19_PMU_PCIE_I
		vin5-supply = <&VREG_L15B_1P8>; // VDD18_AON
		vin6-supply = <&VREG_L3C_1P2>; // VDD12_18_IO
		voltage-list = <916000 944000 976000 1316000 1864000 1800000 1200000>;
		// l6k: 1800000
		// s1c: 1316000
		// s6c: 1864000
		// s3c: 916000
		// s2c: 976000
		// s4i: 940000 (rounded to 944000 because of driver problem)
		// l3c: 1200000
		// l15b: 1800000

		clocks = <&rpmhcc RPMH_RF_CLK1>;
		clock-names = "TCXOI";
	};

	dummy_codec: dummy_codec {
		compatible = "linux,snd-soc-dummy";
		#sound-dai-cells = <0>;
	};

	sbu-mux {
		compatible = "gpio-sbu-mux";

		enable-gpios = <&tlmm 86 GPIO_ACTIVE_LOW>;
		select-gpios = <&tlmm 82 GPIO_ACTIVE_HIGH>;

		mode-switch;
		orientation-switch;

		port {
			sbu_mux_in: endpoint {
				remote-endpoint = <&tcpm_sbu_out>;
			};
		};

		// XXX: needs VREG_L05B_3P1, VREG_BOB2
	};

	VREG_6DOF_1V8 {
		compatible = "regulator-fixed2";
		gpios = <&tlmm 36 GPIO_ACTIVE_HIGH>,
			<&tlmm 33 GPIO_ACTIVE_HIGH>;
		r-supply = <&VREG_L5M_1P8>;
		l-supply = <&VREG_L5N_1P8>;

		VREG_6DOF_1V8_R: r0 {
			regulator-name = "6DOF_1V8_R";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};
		VREG_6DOF_1V8_UR: r1 {
			regulator-name = "6DOF_1V8_UR";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};
		VREG_6DOF_1V8_L: l0 {
			regulator-name = "6DOF_1V8_L";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};
		VREG_6DOF_1V8_UL: l1 {
			regulator-name = "6DOF_1V8_UL";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};
	};

	VREG_6DOF_1V2 {
		compatible = "regulator-fixed2";
		gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>,
			<&tlmm 34 GPIO_ACTIVE_HIGH>;
		r-supply = <&VREG_L2M_1P2>;
		l-supply = <&VREG_L2N_1P2>;

		VREG_6DOF_1V2_R: r0 {
			regulator-name = "6DOF_1V2_R";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
		};
		VREG_6DOF_1V2_UR: r1 {
			regulator-name = "6DOF_1V2_UR";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
		};
		VREG_6DOF_1V2_L: l0 {
			regulator-name = "6DOF_1V2_L";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
		};
		VREG_6DOF_1V2_UL: l1 {
			regulator-name = "6DOF_1V2_UL";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
		};
	};

	EYETCK_DOVDD_1V8: EYETCK_DOVDD_1V8 {
		compatible = "regulator-fixed";
		regulator-name = "EYETCK_DOVDD_1V8";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		enable-active-high;
		gpio = <&tlmm 74 GPIO_ACTIVE_HIGH>;
		vin-supply = <&VREG_L5M_1P8>;
	};


	gpio-keys {
		compatible = "gpio-keys";

		pinctrl-0 = <&volume_up_n>;
		pinctrl-names = "default";

		key-volume-up {
			label = "Volume Up";
			linux,code = <KEY_VOLUMEUP>;
			gpios = <&pm8550_gpios 6 GPIO_ACTIVE_LOW>;
			debounce-interval = <15>;
			linux,can-disable;
			wakeup-source;
		};
	};

	BACKLIGHT: backlight {
		compatible = "mpq3367-backlight";
		preboost-gpio = <&tlmm 41 GPIO_ACTIVE_HIGH>;
		en-gpios = <&pm8550_gpios 1 GPIO_ACTIVE_HIGH>,
			   <&pm8550_gpios 2 GPIO_ACTIVE_HIGH>;
		bias-gpios = <&pm8550_gpios 4 GPIO_ACTIVE_HIGH>,
			     <&pm8550_gpios 9 GPIO_ACTIVE_HIGH>;
		i2c-adapter = <&i2c4 &i2c5>;
	};

		FPGA_clk: FPGA_clk {
		compatible = "fpga-clock";
		#clock-cells = <0>;
		clock-output-names = "fpga_clk";
		status = "okay";

		pinctrl-names = "default";
		pinctrl-0 = <&FPGA_clk_default>;

		clocks = <&gcc GCC_GP3_CLK>;
		clock-frequency = <33333333>;
		clock-names = "pinclk";
	};

	fpga: fpga {
		compatible = "efinix,t8f81c2";

		clocks = <&FPGA_clk>;
		clock-names = "fpga_clk";
		v1v1-supply = <&VREG_L2G_1P1>;
		pwren-gpio = <&tlmm 75 GPIO_ACTIVE_HIGH>;
	};

	exp_vout_5v: exp_vout_5v {
		compatible = "regulator-fixed";
		regulator-name = "exp_vout_5v";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&pm8550vs_g_gpios 6 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		vin-supply = <&VPH_PWR>;
		regulator-always-on;
	};

};

&pmk8550_vadc {
	// NOTE TODO REVISIT - There is some chance to lower interrupt usage slightly if these are converted to thermal zones and qcom,adc-tm-type is set appropriately..

	// NOTE Setting qcom,adc-tm-type errors out driver and blocks all channels with obscure error. Need Thermal Zone setup to match.
	pmk8550_xo_therm {
		reg = <PMK8550_ADC5_GEN3_AMUX_THM1_XO_THERM_100K_PU>;
		label = "pmk8550_xo_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};


	/* External PM8550 Thermistors */
	// 200 us measured on POC-F Boards as required time to reach stability.
	pcba_at_fan_exhaust {
		// Quiet Therm RT0502
		reg = <PM8550_ADC5_GEN3_AMUX_THM1_100K_PU>;
		label = "pcba_at_fan_exhaust";
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};

	pcba_at_40pin_connector {
		// SDM SKIN Therm RT507
		reg = <PM8550_ADC5_GEN3_AMUX_THM2_100K_PU>;
		label = "pcba_at_40pin_connector";
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};

	pcba_at_wifi {
		// WLAN Therm RT0503
		reg = <PM8550_ADC5_GEN3_AMUX_THM3_100K_PU>;
		label = "pcba_at_wifi";
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};

	pcba_at_flash {
		// ADC_1 Therm RT506
		reg = <PM8550_ADC5_GEN3_AMUX_THM4_100K_PU>;
		label = "pcba_at_flash";
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};

	pcba_at_nrf {
		// ADC_2 Therm RT505
		reg = <PM8550_ADC5_GEN3_AMUX_THM5_100K_PU>;
		label = "pcba_at_nrf";
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};


	/* External PM8550B Thermistors */
	pcba_at_heat_sink_fins {
		reg = <PM8550B_ADC5_GEN3_AMUX_THM4_USB_THERM_100K_PU>;
		label = "pcba_at_heat_sink_fins";
		qcom,hw-settle-time = <200>;
		qcom,ratiometric;
	};
};

// Different scaling for 2S
// TGOYEN - I am not fully sure how this comes about.
// It definitely doesn't match the values expected by the driver.
// I found documentation mapping 28,848 counts = 12 Amps.
// However, this is wrong since we use 4.22k for ICHG_FB which makes 60 mA steps
// So the correct mapping is 28,848 * 50 / 60 = 24,040 counts = 12 Amps
// Mapping is then 12,000,000 / 24,040
// NOTE This scaling has to be added to qcom-vadc-common.c as well
&pm8550b_ichg_fb {
	qcom,pre-scaling = <24040 12000000>;
};

