		l-supply = <&vreg_l5n_1p8>;

		vreg_6dof_1p8_r: r0 {
			regulator-name = "vreg_6dof_1p8_r";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};

		vreg_6dof_1p8_ur: r1 {
			regulator-name = "vreg_6dof_1p8_ur";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};

		vreg_6dof_1p8_l: l0 {
			regulator-name = "vreg_6dof_1p8_l";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};

		vreg_6dof_1p8_ul: l1 {
			regulator-name = "vreg_6dof_1p8_ul";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};
	};

	vreg_6dof_1p2 {
		compatible = "regulator-fixed2";
		gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>,
			<&tlmm 34 GPIO_ACTIVE_HIGH>;
		r-supply = <&vreg_l2m_1p2>;
		l-supply = <&vreg_l2n_1p2>;

		vreg_6dof_1p2_r: r0 {
			regulator-name = "vreg_6dof_1p2_r";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
		};

		vreg_6dof_1p2_ur: r1 {
			regulator-name = "vreg_6dof_1p2_ur";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
		};

		vreg_6dof_1p2_l: l0 {
			regulator-name = "vreg_6dof_1p2_l";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
		};

		vreg_6dof_1p2_ul: l1 {
			regulator-name = "vreg_6dof_1p2_ul";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
		};
	};

	eyetck_dovdd_1v8: eyetck_dovdd_1v8 {
		compatible = "regulator-fixed";
		regulator-name = "eyetck_dovdd_1v8";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		enable-active-high;
		gpio = <&tlmm 74 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vreg_l5m_1p8>;
	};


	gpio-keys {
		compatible = "gpio-keys";

		pinctrl-0 = <&volume_up_n>;
		pinctrl-names = "default";

		key-volume-up {
			label = "Volume Up";
			linux,code = <KEY_VOLUMEUP>;
			gpios = <&pm8550_gpios 6 GPIO_ACTIVE_LOW>;
			debounce-interval = <15>;
			linux,can-disable;
			wakeup-source;
		};
	};

	backlight: backlight {
		compatible = "mpq3367-backlight";
		preboost-gpio = <&tlmm 41 GPIO_ACTIVE_HIGH>;
		en-gpios = <&pm8550_gpios 1 GPIO_ACTIVE_HIGH>,
			   <&pm8550_gpios 2 GPIO_ACTIVE_HIGH>;
		bias-gpios = <&pm8550_gpios 4 GPIO_ACTIVE_HIGH>,
			     <&pm8550_gpios 9 GPIO_ACTIVE_HIGH>;
		i2c-adapter = <&i2c4gpio &i2c5gpio>;
	};

	fpga_clk: fpga_clk {
		compatible = "fpga-clock";
		#clock-cells = <0>;
		clock-output-names = "fpga_clk";
		status = "okay";

		pinctrl-0 = <&fpga_clk_default>;
		pinctrl-names = "default";

		clocks = <&gcc GCC_GP3_CLK>;
		clock-frequency = <33333333>;
		clock-names = "pinclk";
	};

	fpga: fpga {
		compatible = "efinix,t8f81c2";

		clocks = <&fpga_clk>;
		clock-names = "fpga_clk";
		v1v1-supply = <&vreg_l2g_1p1>;
		pwren-gpio = <&tlmm 75 GPIO_ACTIVE_HIGH>;
	};

	exp_vout_5v: exp_vout_5v {
		compatible = "regulator-fixed";
		regulator-name = "exp_vout_5v";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&pm8550vs_g_gpios 6 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		vin-supply = <&vph_pwr>;
		regulator-always-on;
	};

	i2c4gpio: i2c4_gpio {
		compatible = "i2c-gpio";
		sda-gpios = <&tlmm 48 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		scl-gpios = <&tlmm 49 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;

		pinctrl-0 = <&i2c4_default>;
		pinctrl-names = "default";

		#address-cells = <1>;
		#size-cells = <0>;
	};

	i2c5gpio: i2c5_gpio {
		compatible = "i2c-gpio";
		sda-gpios = <&tlmm 52 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		scl-gpios = <&tlmm 53 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;

		pinctrl-0 = <&i2c5_default>;
		pinctrl-names = "default";

		#address-cells = <1>;
		#size-cells = <0>;
	};
};

