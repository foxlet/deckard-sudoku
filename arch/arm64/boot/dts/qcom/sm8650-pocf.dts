/ {
    model = "SM8650 POC-F rev1 4slam 2et";

	interrupt-parent = <&intc>;

	#address-cells = <2>;
	#size-cells = <2>;

	aliases {
		serial0 = &uart15;
		serial1 = &uart14;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";

		regulator-name = "vph_pwr";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;

		regulator-always-on;
		regulator-boot-on;
	};

	vdd_gfx: vdd-gfx {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_GFX>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "GFX";
		regulator-always-on;
	};

	vdd_mxc: vdd-mxc {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_MXC>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "MXC";
		regulator-always-on;
	};

	vdd_lcx: vdd-lcx {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_LCX>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "LCX";
		regulator-always-on;
	};

	vdd_lmx: vdd-lmx {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_LMX>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "LMX";
		regulator-always-on;
	};

	vdd_nsp: vdd-nsp {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_NSP>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "NSP";
		regulator-always-on;
	};

	vdd_nsp2: vdd-nsp2 {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_NSP2>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "NSP2";
		regulator-always-on;
	};

	vdd_mx: vdd-mx {
		compatible = "regulator-fixed-domain";
		power-domains = <&rpmhpd RPMHPD_MX>;
		required-opps = <&rpmhpd_opp_nom>;
		regulator-name = "MX";
		regulator-always-on;
	};


	VPH_PWR: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "VPH_PWR";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;

		regulator-always-on;
		regulator-boot-on;
	};

